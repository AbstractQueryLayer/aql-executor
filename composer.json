{
  "name": "ifcastle/aql-executor",
  "description": "AQL Query executor component.",
  "keywords": [
    "aql",
    "query",
    "language",
    "storage"
  ],
  "type": "ifcastle-aql-package",
  "license": "MIT",
  "authors": [
    {
      "name": "Edmond Dantes",
      "email": "edmondifthen@proton.me"
    }
  ],
  "require": {
    "php": ">=8.5",
    "ifcastle/aql-aspects": "*",
    "ifcastle/aql-dsl": "*",
    "ifcastle/aql-entity": "dev-main",
    "ifcastle/aql-result": "*",
    "ifcastle/aql-storage": "*",
    "ifcastle/aql-transaction": "*",
    "ifcastle/design-patterns": "^0.2.20",
    "ifcastle/di": "^0.6.0",
    "ifcastle/exceptions": "^5.0",
    "ifcastle/type-definitions": "^0.5.7"
  },
  "autoload": {
    "psr-4": {
      "IfCastle\\AQL\\Executor\\": [
        "src/"
      ]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "IfCastle\\AQL\\Executor\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^11.2",
    "rector/rector": "^1.2",
    "ifcastle/codestyle": "^0.2.0",
    "ifcastle/aql-test-utils": "*"
  },
  "repositories": [
    {
      "type": "path",
      "url": "../aql-entity"
    }
  ],
  "scripts": {
    "test": "vendor/bin/phpunit ./tests",
    "cs-fix": "php-cs-fixer fix",
    "cs-check": "php-cs-fixer fix --dry-run --diff",
    "rector": "rector process",
    "rector-dry": "rector process --dry-run"
  },
  "extra": {
    "branch-alias": {
      "dev-main": "0.8.x-dev"
    }
  }
}
